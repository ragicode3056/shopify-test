<h1>My Orders</h1>

{% assign orders = shopify_api.order.all(limit: 10) %}

<table>
  <thead>
    <tr>
      <th>Order Number</th>
      <th>Date</th>
      <th>Customer</th>
      <th>Total</th>
    </tr>
  </thead>
  <tbody>
    {% for order in orders %}
      <tr>
        <td>{{ order.order_number }}</td>
        <td>{{ order.created_at | date: "%b %d, %Y" }}</td>
        <td>{{ order.customer.name }}</td>
        <td>{{ order.total_price | money }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>
